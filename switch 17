#include <stdio.h>
int main() {
    int type,units;
    scanf("%d %d",&type,&units);
    switch(type){
        case 1:
        if(units<=30){
            printf("%d",units*5);
        }else{
            printf("%d",(((units-30)*8)+( (units=30*5))));
        }
        break;
        case 2:
        printf("%d",units*10);
        break;
    }
    
    return 0;
}
