#include <stdio.h>
int main() {
    int type;
    float usage;
    scanf("%d %f",&type,&usage);
    switch(type){
        case 1:
        if(usage<=1){
            printf("Normal Speed");
        }else{
            printf("Speed Reduced");
        }break;
        case 2:
        if(usage<=2){
            printf("Normal Speed");
        }else if(usage>2){
            printf("Extra Charges Applied");
        }break;
        default:
        printf("Extra Charges Applied");
        break;
    }
     return 0;
}
